<process>
  <phtag_clean enable="yes"/>
  <url_clean enable="yes"/>
  <tokenize enable="yes"/>
  <lowercase enable="yes"/>
  <num_clean enable="yes"/>
  <dup_clean enable="yes">
    <restricted>no</restricted>
  </dup_clean>
  <extra_long_clean enable="yes">
    <source>50</source>
    <target>50</target>
  </extra_long_clean>
  <weird_align_clean enable="yes">
    <diff>20</diff>
  </weird_align_clean>
</process>